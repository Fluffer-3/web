        "autoprefixer": "^10.4.20",
        "electron": "^31.3.1",
        "electron-builder": "^24.13.3",
        "electron-vite": "^2.3.0",
        "eslint": "^9.8.0",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.9",
        "less": "^4.2.0",
        "postcss": "^8.4.41",
        "postcss-preset-mantine": "^1.17.0",
        "postcss-simple-vars": "^7.0.1",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.9",
