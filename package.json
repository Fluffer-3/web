        "moment": "^2.30.1",
        "ms": "^2.1.3",
        "react": "^18.3.1",
        "react-avatar-edit": "^1.2.0",
        "react-dom": "^18.3.1",
        "react-icons": "^5.2.1",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.26.0",
        "recharts": "2",
        "redux-persist": "^6.0.0",
