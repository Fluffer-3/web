        "@iconify/icons-bi": "^1.2.19",
        "@iconify/react": "^5.0.2",
        "@mantine/carousel": "^7.12.0",
        "@mantine/charts": "^7.12.0",
        "@mantine/code-highlight": "^7.12.0",
        "@mantine/core": "^7.12.0",
        "@mantine/dates": "^7.12.0",
        "@mantine/dropzone": "^7.12.0",
        "@mantine/form": "^7.12.0",
        "@mantine/hooks": "^7.12.0",
        "@mantine/modals": "^7.12.0",
        "@mantine/notifications": "^7.12.0",
        "@mantine/nprogress": "^7.12.0",
        "@mantine/spotlight": "^7.12.0",
        "@mantine/tiptap": "^7.12.0",
        "@reduxjs/toolkit": "^2.2.7",
        "@tabler/icons-react": "^3.12.0",
        "@tiptap/extension-link": "^2.5.9",
        "@tiptap/react": "^2.5.9",
        "@tiptap/starter-kit": "^2.5.9",
        "apollo-upload-client": "^18.0.1",
        "classnames": "^2.5.1",
        "clsx": "^2.1.1",
